<h1>Dashboard#index</h1>
<p>Find me in app/views/dashboard/index.html.erb</p>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Group</th>
    </tr>
  </thead>
  <tbody>
  <% @groups.each do |group| %>
    <tr>
      <td><%= group.name %></td>
      <td><%= link_to 'new group schedule', group_schedule_new_path(group.id) %></td>
    </tr>
  <% end %>
  </tbody>
</table>
